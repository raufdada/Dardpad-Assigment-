  List<int> numbers = [8, 2, 5, 1, 9, 4, 6, 3, 7];

  int smallest = numbers[0]; 
  int greatest = numbers[0]; 
  for (int number in numbers) {
    if (number < smallest) {
      smallest = number; 
    }
    if (number > greatest) {
      greatest = number;     }
  }
  print("Smallest number: $smallest");
  print("Greatest number: $greatest");
}

  Ans:Smallest number: 1
Greatest number: 9
